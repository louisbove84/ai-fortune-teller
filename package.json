{
  "name": "ai-fortune-teller",
  "version": "1.0.0",
  "private": true,
  "description": "AI Fortune Teller - Farcaster Mini App for career resilience assessment",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "postinstall": "node -e \"try { require('husky').install() } catch (e) {}\""
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "husky": "^9.0.11",
    "prettier": "^3.3.3",
    "eslint": "^8.57.0",
    "typescript": "^5.5.0"
  },
  "packageManager": "npm@10.8.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "overrides": {
    "react": "^19.0.0-rc",
    "react-dom": "^19.0.0-rc"
  }
}

